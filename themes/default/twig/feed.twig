<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
<channel>
  <title>Repositories | Brandon Rozek</title>
  <description>Repositories made by Brandon Rozek</description>
  <link>https://repos.brandonrozek.com</link>
  <atom:link href="https://repos.brandonrozek.com/feed/" rel="self" type="application/rss+xml" />
  {% for repository in repositories %}
    <item>
      <title>{{ repository.name }}</title>
      <description>{{ repository.description }}</description>
      <link>https://repos.brandonrozek.com{{ path('repository', {repo: repository.name}) }}</link>
      <guid isPermaLink="true">https://repos.brandonrozek.com{{ path('repository', {repo: repository.name}) }}</guid>
    </item>
  {% endfor %}
</channel>
</rss>
